public class AnalogPair {

	int x;
	int y;
	
	public AnalogPair() {
		x = -1;
		y = -1;
	}
	
	public AnalogPair(int myX, int myY) {
		x = myX;
		y = myY;
	}
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
	}
	
	public void setX(int myX) {
		x = myX;
	}
	
	public void setY(int myY) {
		y = myY;
	}
	
	public void set(int myX, int myY) {
		x = myX;
		y = myY;
	}
	
	public String toString() {
		return x + " " + y;
	}
}